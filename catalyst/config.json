{
  "$schema": "../catalyst.schema.json",
  "back": {
    "key-sequence": [
      "CONTROL",
      "MAINMOD",
      "left"
    ],
    "command": "playerctl previous"
  },
  "pause": {
    "key-sequence": [
      "CONTROL",
      "MAINMOD",
      "p"
    ],
    "command": "playerctl play-pause"
  },
  "next": {
    "key-sequence": [
      "CONTROL",
      "MAINMOD",
      "right"
    ],
    "command": "playerctl next"
  },
  "reload": {
    "key-sequence": [
      "CONTROL",
      "SUPER",
      "E"
    ],
    "command": "eww reload"
  }
}
